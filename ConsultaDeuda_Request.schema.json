{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
        "id_empresa",
        "tipo_busqueda",
        "valor_busqueda",
        "cod_recaudador",
        "cred_recaudador"
    ],
    "properties": {
        "id_empresa": {
            "type": "integer",
            "minLength": 1,
            "maxLength": 1
        },
        "tipo_busqueda": {
            "type": "string",
            "maxLength": 25
        },
        "valor_busqueda": {
            "type": "string",
            "maxLength": 100
        },
        "cod_recaudador": {
            "type": "string",
            "maxLength": 20
        },
        "cred_recaudador": {
            "type": "string",
            "maxLength": 200
        },
        "cod_oficina_recaudador": {
            "type": "string",
            "maxLength": 4
        },        
        "cod_caja_recaudador": {
            "type": "string",
            "maxLength": 4
        }
    }
}
